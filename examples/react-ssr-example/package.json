  "description": "Example project using the framework",
  "homepage": "https://github.com/pixeloven/pixeloven",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixeloven/pixeloven.git"
  },
  "author": "Brian Gebel",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@pixeloven/react-router-config": "^6.0.0-alpha.16",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "express": "4.16.4",
    "express-winston": "3.1.0",
    "immer": "1.8.0",
    "moment": "2.24.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-form": "7.4.2",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "semantic-ui-react": "0.86.0",
    "semantic-ui-sass": "2.3.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@pixeloven/cli": "^6.0.0-alpha.16",
    "@pixeloven/cli-addon-storybook": "^6.0.0-alpha.16",
    "@pixeloven/cli-addon-webpack": "^6.0.0-alpha.16",
    "@pixeloven/generators": "^6.0.0-alpha.16",
    "@pixeloven/storybook": "^6.0.0-alpha.16",
    "@pixeloven/webpack": "^6.0.0-alpha.16",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/express": "4.16.1",
    "@types/express-winston": "3.0.0",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-redux": "7.0.8",
    "@types/react-router": "4.4.5",
    "@types/react-router-dom": "4.3.3",
    "@types/supertest": "2.0.7",
    "@types/webpack": "4.4.32",
    "babel-loader": "8.0.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "node-mocks-http": "1.7.3",
    "npm-run-all": "4.1.5",
    "redux-saga-test-plan": "3.7.0",
    "supertest": "3.3.0"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "scripts": {
    "cli": "pixeloven",
    "build": "yarn build:app && yarn build:story",
    "build:app": "pixeloven webpack build --profile --source-map --path /example/",
    "build:story": "pixeloven story build",
    "prebuild": "pixeloven delete dist",
    "document": "pixeloven document ts ./src",
    "predocument": "pixeloven delete docs",
    "generate": "pixeloven-generators generate",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "pixeloven lint ts ./src/**/*.{ts,tsx}",
    "lint:scss": "pixeloven lint scss ./src/**/*.scss",
    "pretest": "pixeloven delete coverage",
    "pretty": "yarn pretty:ts && yarn pretty:scss",
    "pretty:ts": "pixeloven pretty ts ./src/**/*.{ts,tsx}",
    "pretty:scss": "pixeloven pretty scss ./src/**/*.scss",
    "start": "run-p start:app start:story",
    "start:app": "NODE_ENV=development pixeloven webpack start --source-map --host 0.0.0.0 --path / --port 8080 --protocol http",
    "start:story": "NODE_ENV=development pixeloven story start",
    "start:prod": "NODE_ENV=production node ./dist/server.js",
    "test": "NODE_ENV=testing pixeloven test --maxWorkers 2 --color --coverage",
    "test:watch": "NODE_ENV=testing pixeloven test watch --color"
  }
}
