{
  "name": "@ssr-frontend/packages-express-asset-middleware",
  "version": "1.24.3",
  "description": "Creates middleware for express to read a manifest file at run time",
  "author": "GoFundMe <eng@gofundme.com>",
  "license": "UNLICENSED",
  "main": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "private",
    "registry": "https://registry.gofundme.npme.io/"
  },
  "dependencies": {
    "@ssr-frontend/packages-util-asset-manifest": "^1.24.3",
    "express": "4.17.1"
  },
  "scripts": {
    "compile": "pixeloven compile ts",
    "document": "pixeloven document ts ./src",
    "lint": "yarn run lint:ts",
    "lint:ts": "pixeloven lint ts ./src/**/*.{ts,tsx}",
    "pretty": "yarn run pretty:ts",
    "pretty:ts": "pixeloven pretty ts ./src/**/*.{ts,tsx}",
    "test": "pixeloven test --color --coverage",
    "test:ci": "pixeloven test --ci",
    "test:watch": "pixeloven test watch",
    "precommit": "lint-staged",
    "predocument": "pixeloven delete docs",
    "pretest": "pixeloven delete coverage"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn pixeloven pretty ts",
      "yarn pixeloven lint ts",
      "git add"
    ]
  }
}
