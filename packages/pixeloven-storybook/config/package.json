{
    "name": "@pixeloven-storybook/config",
    "version": "6.3.1",
    "description": "Storybook configuration",
    "main": "dist/lib/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "homepage": "https://www.pixeloven.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pixeloven/pixeloven.git"
    },
    "author": "Brian Gebel",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@babel/core": "7.10.1",
        "@pixeloven-core/filesystem": "^6.3.1",
        "@pixeloven-webpack/ts-loader": "^6.3.1",
        "@pixeloven-webpack/types": "^6.1.0",
        "@storybook/addon-a11y": "5.3.21",
        "@storybook/addon-actions": "5.3.21",
        "@storybook/addon-backgrounds": "5.3.21",
        "@storybook/addon-knobs": "5.3.21",
        "@storybook/addon-notes": "5.3.21",
        "@storybook/addon-options": "5.3.21",
        "@storybook/addon-viewport": "5.3.21",
        "@storybook/addons": "5.3.21",
        "@storybook/core": "5.3.21",
        "@storybook/react": "5.3.21",
        "@storybook/theming": "5.3.21",
        "babel-loader": "8.1.0",
        "css-loader": "3.4.2",
        "deepmerge": "4.2.2",
        "fork-ts-checker-webpack-plugin": "4.1.6",
        "sass": "1.26.2",
        "sass-loader": "8.0.2",
        "style-loader": "1.1.2",
        "tsconfig-paths-webpack-plugin": "3.2.0",
        "webpack": "4.41.5"
    },
    "devDependencies": {
        "@pixeloven-tooling/compiler": "^6.2.0",
        "@pixeloven-tooling/linter": "^6.3.1",
        "@pixeloven-tooling/pretty": "^6.2.0",
        "@pixeloven-tooling/test": "^6.2.0",
        "@types/mini-css-extract-plugin": "0.8.0",
        "lint-staged": "10.4.2",
        "react": "16.13.1",
        "react-dom": "16.13.1"
    },
    "peerDependencies": {
        "react": "16.x",
        "react-dom": "16.x"
    },
    "scripts": {
        "compile": "pixeloven-compiler ts",
        "compile:watch": "pixeloven-compiler ts --watch",
        "lint": "pixeloven-linter ts src/**/*.{ts,tsx}",
        "lint:ts": "pixeloven-linter ts src/**/*.{ts,tsx}",
        "pretty": "pixeloven-pretty **/*.{json,md,ts,tsx}",
        "precommit": "lint-staged",
        "test": "pixeloven-test --color --coverage",
        "test:ci": "pixeloven-test --ci --coverage",
        "test:watch": "pixeloven-test --watch"
    },
    "lint-staged": {
        "*.{json,md}": [
            "pixeloven-pretty"
        ],
        "src/**/*.{ts,tsx}": [
            "pixeloven-pretty",
            "pixeloven-linter ts --fix",
            "pixeloven-linter ts"
        ]
    }
}
