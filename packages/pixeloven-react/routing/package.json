    "@types/react": "16.8.23",
    "@types/react-redux": "7.1.1",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "lint-staged": "9.2.5",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
