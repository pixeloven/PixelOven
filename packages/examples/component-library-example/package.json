{
  "name": "@examples/component-library-example",
  "version": "6.0.0-beta.10",
  "description": "Component example for creating a React + TypeScript based library",
  "main": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/pixeloven/pixeloven",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixeloven/pixeloven.git"
  },
  "author": "Brian Gebel",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@pixeloven/cli": "^6.0.0-beta.10",
    "@pixeloven/cli-addon-storybook": "^6.0.0-beta.10",
    "@pixeloven/cli-addon-webpack": "^6.0.0-beta.11",
    "copyfiles": "2.1.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "lint-staged": "9.2.5",
    "react": "16.11.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "scripts": {
    "compile": "pixeloven webpack build --library ./src/index.ts --stats --source-map",
    "document": "pixeloven document ts ./src",
    "lint": "yarn run lint:ts",
    "lint:ts": "pixeloven lint ts 'src/**/*.{ts,tsx}'",
    "precompile": "pixeloven delete dist",
    "precommit": "lint-staged",
    "predocument": "pixeloven delete docs",
    "pretest": "pixeloven delete coverage",
    "pretty": "yarn run pretty:ts && yarn run pretty:scss",
    "pretty:ts": "pixeloven pretty ts 'src/**/*.{ts,tsx}'",
    "pretty:scss": "pixeloven pretty scss 'src/**/*.scss'",
    "test": "pixeloven test --color --coverage",
    "test:ci": "pixeloven test --ci --coverage",
    "test:watch": "pixeloven test watch"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn pixeloven pretty ts",
      "yarn pixeloven lint ts",
      "git add"
    ]
  }
}
