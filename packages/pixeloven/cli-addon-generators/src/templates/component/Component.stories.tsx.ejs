import {storiesOf} from "@storybook/react";
import React from "react";

import {<%= props.name %>} from "./index";
import Readme from "./README.md";

storiesOf("<% if (props.nameSpace) { %><%= props.nameSpace %>/<% } %><%= props.atomicType %>s/<%= props.name %>", module)
    .add("default", () => {
        return <<%= props.name %> className={"story-class"} />;
    }, {
        notes: { markdown: Readme },
    });
